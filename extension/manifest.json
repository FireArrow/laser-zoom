{
  "manifest_version": 2,

  "name": "Laser-zoom",
  "description": "This extension allows web pages react to lasers from dotdetector",
  "version": "1.0",

  "minimum_chrome_version": "23",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title":"pew pew"
  },
  "background": {
      "scripts": [ "ddclient.js" ]
  },
  "permissions": [
      "tabs", "http://*/*", "https://*/*"
  ]
}
